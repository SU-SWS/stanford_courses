<?php

/**
 * @file
 * su_course_tags.module
 */

/**
 * Implements hook_page_attachments().
 */
function stanford_courses_page_attachments(array &$attachments) {
    // Get the route
    $route = \Drupal::routeMatch()->getRouteName();
    if (str_contains($route, 'page_view_courses_list_page_courses_list_page')) {
        $attachments['#attached']['library'][] = 'stanford_courses/courses_page';
    }
}


